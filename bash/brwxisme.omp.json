
{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [ 
    {
      "alignment": "left",
      "newline": true,
      "segments": [
      {
          "background": "#a48cf2",
          "foreground": "#212337",
          "leading_diamond": " ",
          "style": "diamond",
          //"leading_diamond": "\ue0b6",
          "leading_diamond": "\n \ue0b6",
          "trailing_diamond": "<transparent,background>\ue0ba",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#a48cf2",
          "foreground": "#212337",
          "properties": {
            "display_host": true
          },
          "style": "diamond",
          "template": "<b>{{if .Root}} \uf0e7 {{.UserName}} {{else}} {{.UserName}} {{end}}</b>",
          "trailing_diamond": "<transparent,background>\ue0ba",
          "leading_diamond": "<transparent,background>\ue0bc",
          "type": "session"
        },
        {
          "background": "#04d1f9",
          "foreground": "#212337",
          //"leading_diamond": "<transparent,background>\ue0b0</>",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "trailing_diamond": "<transparent,background>\ue0ba",
          "leading_diamond": "<transparent,background>\ue0bc",
          "template": " \ue5ff {{ .Path }} ",
          //"trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#37f499",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#f265b5{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#e43b44{{ end }}",
            "{{ if gt .Ahead 0 }}#f1fc79{{ end }}",
            "{{ if gt .Behind 0 }}#2ce8f5{{ end }}"
          ],
          "foreground": "#262b44",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "trailing_diamond": "<transparent,background>\ue0ba",
          "leading_diamond": "<transparent,background>\ue0bc",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{if .BranchStatus }} {{ .BranchStatus }} {{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} | {{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background_templates": [
            "{{ if gt .Code 0 }}#f16c75{{else}}#37f499{{ end }}"
          ],
          "foreground_templates": [
            "{{ if gt .Code 0 }}#f16c75{{else}}#37f499{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "foreground": "#7081d0",
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0bc",
          //"leading_diamond": "<transparent,background>â—¤",
          "trailing_diamond": "\ue0b0",
          "template": "\ue0b0",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#37f499",
          "background_templates": [
            "{{ if gt .Code 0 }}#f16c75{{ end }}"
          ],
          "foreground": "#212337",
          //"leading_diamond": "<transparent,background>\ue0b0</>",
          "leading_diamond": "\ue0b6",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": "{{ if gt .Code 0 }} âœ— {{else}} <b>ðŸ—¸</b> {{ end }}",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,background>\ue0be",
          "type": "status"
        },
        {
          "background": "#a48cf2",
          "foreground": "#212337",
          "properties": {
            "style": "austin",
            "threshold": 0
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b8",
          "template": " <#fefefe>\udb86\udd9f</> {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#37f499",
          "style": "plain",
          //"template": "\n",
          //"template": "\u2800",
          "template":"î°ƒ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
