"memory": {
    "interval": 3,
    "format": "ÓøÖ {percentage}% ",
    "max-length": 10
},
"cpu": {
    "interval": 3,
    "format": "Ôãõ {usage}% ",
},
"tray": {
        "icon-size": 14,
        "rotate": 0,
        "spacing": 6,
        "show-passive-items":true,
},

"group/expand": {
     "orientation": "horizontal",
     "modules": ["tray"]
},

 
"hyprland/workspaces": {
	"format": "{icon}",
	"format-icons": {
	"default": "",
        "empty": "",
	"active": "",
	// "default": "ÔÜí",
	//        "empty": "ÔÑå",
	// "active": "ÔÑë",
	},
  "persistent-workspaces": {
             "DP-2": 5, // 5 workspaces by default on every monitor
